{
  "extends": "airbnb-base",
  "env": {
    "mocha": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "max-len": "off",
    "no-use-before-define": "off",
    "import/named": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "no-unused-vars": "off",
    "no-shadow": "off"
  }
}